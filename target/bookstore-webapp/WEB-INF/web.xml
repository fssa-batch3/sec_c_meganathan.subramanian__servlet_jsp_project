<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>Combobook</servlet-name>
		<display-name>Combobook</display-name>
		<description></description>
		<servlet-class>com.fssa.bookstore.userservlet.Combobook</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Logout</servlet-name>
		<display-name>Logout</display-name>
