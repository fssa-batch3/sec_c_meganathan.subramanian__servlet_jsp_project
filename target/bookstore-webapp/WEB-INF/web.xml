		<servlet-class>com.fssa.bookstore.userservlet.Logout</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ProfileServlet</servlet-name>
		<display-name>ProfileServlet</display-name>
		<description></description>
		<servlet-class>com.fssa.bookstore.userservlet.ProfileServlet</servlet-class>
